	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>pe.com.claro.vent</groupId>
		<artifactId>claro-vent-pinporta</artifactId>
		<version>1.0.0</version>
	</parent>
	<artifactId>claro-vent-pinporta-canonical</artifactId>
